  /gene2phenotype/api/reference_data/:
    get:
      operationId: reference_data_retrieve
      description: |2

        Fetch list of all reference data used in G2P with their respective versions.
      tags:
      - Reference data
      security:
      - jwtAuth: []
      - {}
      responses:
        '200':
          content:
            application/json:
              schema:
                type: array
                items:
                  type: object
