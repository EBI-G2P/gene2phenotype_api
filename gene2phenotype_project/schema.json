                  panel_name:
                    type: string
                  records_summary:
                    type: object
                    properties:
                      locus:
                        type: string
                      disease:
                        type: string
                      genotype:
                        type: string
                      confidence:
                        type: string
                      variant_consequence:
                        type: array
                        items:
                          type: string
                      variant_type:
                        type: array
                        items:
                          type: string
                      molecular_mechanism:
                        type: string
                      last_updated:
                        type: string
                      stable_id:
                        type: string
